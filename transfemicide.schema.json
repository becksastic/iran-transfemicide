{
  "fields": [
    {
      "name": "case_id",
      "type": "string",
      "constraints": {
        "required": true,
        "pattern": "^([A-Z]{2})-[A-Z]{2,3}-\\d{4}-\\d{3}$"
      }
    },
    {
      "name": "victim_name",
      "type": "string",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "age",
      "type": "integer",
      "constraints": {
        "minimum": 1,
        "maximum": 120
      }
    },
    {
      "name": "gender_identity",
      "type": "string",
      "constraints": {
        "required": true,
        "enum": [
          "Trans woman",
